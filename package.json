  "scripts": {
    "runAll": "nx run-many --target=serve --projects=auth,tickets,notifications --parallel",
    "runAuth": "npx nx serve auth",
    "runTickets": "npx nx serve tickets",
    "runNotifications": "npx nx serve notifications",
    "dockerUp": "docker-compose up -d",
